<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="realtime-deviations-v1">
    <Description></Description>
    <FaultRules/>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <FaultRules/>
                <Name>flowCallout.globalIpBlacklistTeamRutedata</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>spikeArrest</Name>
            </Step>
            <Step>
                <FaultRules/>
                <Name>flowCallout.getEnvironmentConfiguration</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <HTTPProxyConnection>
        <BasePath>/realtime-deviations/v1</BasePath>
        <Properties/>
        <VirtualHost>entur</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="realtime-deviations-v1">
        <TargetEndpoint>realtime-deviations-v1</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
